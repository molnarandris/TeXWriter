<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="ResultViewer" parent="GtkStack">
    <child>
      <object class="GtkStackPage">
        <property name="name">pdf</property>
        <property name="child">
          <object class="PdfViewer" id="pdfview">
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">log</property>
        <property name="child">
          <object class="LogViewer" id="logview">
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
